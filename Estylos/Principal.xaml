        <Style x:Key="TituloVentana" TargetType="Label">
            <Setter Property="FontFamily" Value="Viner Hand ITC"/>
            <Setter Property="HorizontalContentAlignment" Value="Left"/>
            <Setter Property="VerticalContentAlignment" Value="Top"/>
            <Setter Property="FontSize" Value="48"/>
            <Setter Property="Foreground" Value="#FFE45E5E"/>
            <Setter Property="Background" Value="{x:Null}"/>
            <Setter Property="RenderTransformOrigin" Value="0.5,0.5"/>
            <Setter Property="Label.RenderTransform">
                <Setter.Value>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform AngleX="-6.016"/>
                        <RotateTransform/>
                        <TranslateTransform X="-1.37"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Setter.Value>
            </Setter>
        </Style>
        <!--  Estiolo para Botones -->
        <Style TargetType="Button">
            <Setter Property="FontFamily" Value="Bernard MT Condensed"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Stretch"/>
            <Setter Property="FontSize" Value="26"/>
            <Setter Property="Foreground" Value="#FFF1F7F7"/>
            <Setter Property="Background" Value="Black"/>
            <Setter Property="RenderTransformOrigin" Value="0.5,0.5"/>
         

        </Style>
    </Window.Resources>

